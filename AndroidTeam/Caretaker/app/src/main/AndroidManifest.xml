<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mlabsystem2.setup_2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PatientMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LocationActivity" />
        <activity android:name=".ScheduleActivity" />
        <activity android:name=".EditDetailsActivity" />
        <activity android:name=".PatientProfile" />
        <activity android:name=".CustomizeMenu"/>
        <activity android:name=".EditContacts"/>
        <activity
            android:name=".Schedulemain"
            android:exported="true" />

        <receiver android:name=".AlarmReceiver" />
        <receiver android:name=".AlarmCancel" />

        <activity android:name=".patient_add" />
        <activity android:name=".taskDescription" />
    </application>

</manifest>